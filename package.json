{
  "name": "milestone-check",
  "version": "1.0.0",
  "description": "Checks whether milestone is set or not in GitHub PRs",
  "author": "Jakub Scholz <www@scholzj.com>",
  "license": "ISC",
  "repository": "https://github.com/scholzj/milestone-check.git",
  "homepage": "https://github.com/scholzj/milestone-check",
  "bugs": "https://github.com/scholzj/milestone-check/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^9.15.1"
  },
  "devDependencies": {
    "jest": "^26.6.1",
    "nock": "^13.0.4",
    "nodemon": "^2.0.6",
    "smee-client": "^1.2.2",
    "standard": "^16.0.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
